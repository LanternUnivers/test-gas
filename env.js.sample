const ENV = {
  DISCORD_ALERT_WEBHOOK: 'YOUR_WEBHOOK_TOKEN',
  LI_MENTION_ROLE_ID: 'YOUR_SERVER_MENTION_ROLE_ID',
};

function seedSecretsOnce() {
  const sp = PropertiesService.getScriptProperties();
  Object.keys(ENV).forEach(k => sp.setProperty(k, String(ENV[k])));
  Logger.log('Secrets saved: %s', Object.keys(ENV).join(', '));
}
